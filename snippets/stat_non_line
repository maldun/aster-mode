# name: stat_non_line
# key: STAT_NON_LINE
# --
STAT_NON_LINE(INFO=${1:1},
              MODELE=${2:Model},
              CHAM_MATER=${3:Material},
              EXCIT=(${4:_F(CHARGE=Boundknt,),
                     _F(CHARGE=Boundary,TYPE_CHARGE='FIXE',FONC_MULT=Fonc),}
                 ),
              ${5:COMP_INCR}
              ${6:COMP_ELASt}
              INCREMENT=_F(LIST_INST=${7:ListInst},),
              ${8:RECH_LINEAIRE}
              ${9:NEWTON}
              ${10:CONVERGENCE}
              ARCHIVAGE=_F(LIST_INST=${11:Archive},
                       ),
              ${12:SOLVEURmult}
	      ${13:CONTACT=Contact,}
          );

