# name: calc_modal
# key: CALC_MODAL
# --
CALC_MODAL(INFO = ${1:1},
           MODELE = ${2:Model},
           CHAM_MATER=${3:Material},
           CHARGE = ${4:Boundary},
           CALC_FREQ=_F(OPTION = ${5:CALC_FREQBande}
	              ),
           );