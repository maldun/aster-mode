# -*- coding: utf-8 -*-
# name: orie_norm_coque
# key: ORIE_NORM_COQUE
# --
ORIE_NORM_COQUE=_F(GROUP_MA='${1:name}',
                   VECT_NORM=${2:(1,1,1,)},
                   ${3:NOEUD or GROUP_NO}='${4:nouds}',
                   ),
