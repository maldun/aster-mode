# -*- coding: utf-8 -*-
# name: defi_group
# key: DEFI_GR
# --
DEFI_GROUP(${1:reuse = Mesh},
           MAILLAGE=${2:Mesh},
	   ${3:CREA_GROUP_MA}=_F(${4:NOM=},
	                         ${5:TOUT='OUI'},
				 ${6:TYPE_MAILLE = '3D',}
	                        ),
	  );
