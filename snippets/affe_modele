# name: affe_modele
# key: AFFE_MODELE
# --
AFFE_MODELE(INFO = ${1:1},
	      MAILLAGE = ${2:Mesh},
	      AFFE = (_F(${3:TOUT = 'OUI'},
	                 PHENOMENE=${4:'MECANIQUE'},
			 MODELISATION=${5:'3D'},
			 ),
		     ),
              );
