# name: assemblage
# key: ASSEMBLAGE
# --
ASSEMBLAGE(INFO=1,
           MODELE=${1:Model},
           CHAM_MATER=${2:Material},
           ${3:CARA_ELEM=Shell},
           CHARGE=(${4:BoundFix},),
           NUME_DDL=CO('${6:NUMEDDL}'),
           MATR_ASSE=(_F(MATRICE=CO('${7:StiffMat}'),OPTION='RIGI_MECA',),
                      _F(MATRICE=CO('${8:MassMat}'),OPTION='MASS_MECA',),
                      ),
           INST=${9:0.0},
           SOLVEUR=_F(RENUM='${10:METIS}',
                      METHODE='${11:MULT_FRONT}',
                      ),
          );