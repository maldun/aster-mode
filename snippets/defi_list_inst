# name: defi_list_inst
# key: DEFI_LIST_INST
# --
DEFI_LIST_INST(DEFI_LIST=_F(METHODE='${1:MANUEL}',
                            LIST_INST=${2:ListReel},
                            ),
               ECHEC=_F(EVENEMENT='${3:ERREUR}',
                        SUBD_PAS=${4:2},
                        SUBD_NIVEAU=${5:4},
                        ),
                );
