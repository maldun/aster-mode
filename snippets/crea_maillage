CREA_MAILLAGE(MAILLAGE=${1:Mesh},
              ${2:LINE_QUAD$$(yas-choose-value '("LINE_QUAD" "MODI_MAILLE" "CREA_GROUP_MA" "CREA_POI1" "QUAD_LINE" "PENTA15_18" "HEXA20_27"))}
              );
