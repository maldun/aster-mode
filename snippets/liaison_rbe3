# name: liaison_rbe3
# group: char
# key: LIAISON_RBE3
# --
LIAISON_RBE3=(_F(GROUP_NO_MAIT='${1:group_ma}',
                 DDL_MAIT=('DX','DY','DZ',
                           'DRX','DRY','DRZ',
                           ),
                           GROUP_NO_ESCL=(${2:'grp_escl'},),
                           DDL_ESCL='DX-DY-DZ',
                           COEF_ESCL=1,
                           ),
            ),