# name: discretel
# key: DISCRETel
# --
DISCRET=(_F(${1:GROUP_MA$$(yas-choose-value '("GROUP_MA" "GROUP_NO" "NOEUD" "GROUP_NO"))}='${2:group_name}',
          ${3:K_T_D_N$$(yas-choose-value '("K_T_D_N" "M_T_D_N" "K_TR_D_N" "M_TR_D_N"))}
           ),
        ),
