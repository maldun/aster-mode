# -*- coding: utf-8 -*-
# name: defi_group_no
# key: DEFI_GROUP_NO
# --
CREA_GROUP_NO=(_F(${4:NOM='Group_1}',
                  ${5:GROUP_MA=('Group_1'),$$(yas-choose-value '("TOUT='OUI'," "GROUP_MA=('Group_1')," "DIFFE=('Grp_1','Grp_2',)," "UNION=('Grp_1','Grp_2',)," "INTERSEC=('Grp_1','Grp_2',),"))},
                  ),
                ),