# name: lire_maillage
# key: LIRE_MAILLAGE
# --
LIRE_MAILLAGE(UNITE = ${1:20},
              FORMAT = ${2:'MED'},
              INFO = ${3:1},
              ${4:INFO_MED=1,}
              );
