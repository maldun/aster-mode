# -*- coding: utf-8 -*-
# name: crea_maillage_tria67
# key: CREA_MAILLAGEtria67
# --
CREA_MAILLAGE(MAILLAGE=${1:Mesh},
              MODI_MAILLE=(_F(TOUT='OUI',
                              OPTION='TRIA6_7',
                              PREF_NOEUD='${2:NT}',
                              ),
                           ),
              );
