# name: calc_modes
# key: CALC_MODES
# --
CALC_MODES(INFO = ${1:1},
           MODELE = ${2:Model},
           ${3:CARA_ELEM =Shell,}
           CHAM_MATER=${4:Material},
           CHARGE = ${5:Boundary},
           ${6:CALC_FREQBande$$(yas-choose-value '("CALC_FREQBande" "CALC_FREQPlus"))}
           );